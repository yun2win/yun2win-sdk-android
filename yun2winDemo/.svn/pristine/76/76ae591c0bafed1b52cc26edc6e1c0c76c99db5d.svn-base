<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/isetting_item_padding_left"
    android:paddingRight="@dimen/isetting_item_padding_right"
    >

    <Button
        android:id="@+id/bt_contactInfo_chat"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dip"
        android:layout_marginTop="15dip"
        android:background="@drawable/nim_message_button_bottom_send_selector"
        android:text="@string/chat"
        android:textColor="@color/white"
        android:textSize="17sp" />

    <Button
        android:id="@+id/bt_contactInfo_delete"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dip"
        android:layout_marginTop="15dip"
        android:background="@drawable/nim_red_round_button_selector"
        android:text="@string/delete_friend"
        android:textColor="@color/white"
        android:textSize="17sp" />

</LinearLayout><!-- From: file:/C:/Users/maa46/Desktop/yun2win-sdk-android-master/yun2win-sdk-android-master/app/src/main/res/layout/contactinfo_footer.xml -->